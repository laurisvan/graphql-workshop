{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "build/index.js",
  "scripts": {
    "build_watch": "tsc -b --watch",
    "build": "tsc -b --incremental",
    "copy_schemas": "cpy **/*schema.graphql ../build/ --parents --cwd=src",
    "dev": "nodemon --watch build --watch schema.graphql --exec ts-node ./src/index.ts",
    "generate_types": "graphql-codegen --config graphql-codegen.yml",
    "restart": "npm run generate_types && npm run build && npm run start",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch": "nodemon --watch build --watch schema.graphql"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@graphql-codegen/cli": "^1.19.4",
    "@graphql-codegen/introspection": "^1.18.1",
    "@graphql-codegen/near-operation-file-preset": "^1.17.13",
    "@graphql-codegen/typescript": "^1.19.0",
    "@graphql-codegen/typescript-operations": "^1.17.12",
    "@graphql-codegen/typescript-react-apollo": "^2.2.1",
    "@graphql-codegen/typescript-resolvers": "^1.18.0",
    "@tsconfig/node14": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^4.9.1",
    "cpy-cli": "^3.1.1",
    "eslint": "^7.15.0",
    "eslint-config-standard-with-typescript": "^19.0.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.1.0",
    "nodemon": "^2.0.6",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@types/bluebird": "^3.5.33",
    "@types/node": "^14.14.11",
    "@types/uuid": "^8.3.0",
    "apollo-server": "^2.19.0",
    "graphql": "^15.4.0",
    "graphql-modules": "^1.0.0",
    "graphql-scalars": "^1.6.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^5.22.3",
    "sequelize-typescript": "^1.1.0",
    "uuid": "^8.3.2"
  }
}
